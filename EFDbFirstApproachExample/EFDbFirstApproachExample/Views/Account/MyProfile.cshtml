@model EFDbFirstApproachExample.Identity.ApplicationUser

@{
    ViewBag.Title = "MyProfile";
}

<h2>MyProfile</h2>
<table class="table-borderless flex-xl-row col-md-12" >
    <tr>
        <td>Username</td>
        <td>@Model.UserName</td>
    </tr>
    <tr>
        <td>Email ID</td>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <td>Phone Number</td>
        <td>@Model.PhoneNumber</td>
    </tr>
    <tr>
        <td>Birthday</td>
        <td>@Model.Birthday</td>
    </tr>
    <tr>
        <td>City</td>
        <td>@Model.city</td>
    </tr>
    <tr>
        <td>Country</td>
        <td>@Model.country</td>
    </tr>
</table>

<a href="/home/Index">Back</a>
